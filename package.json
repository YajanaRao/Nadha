{
  "name": "nadha",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "lerna": "node_modules/.bin/lerna",
    "watch": "node_modules/.bin/lerna run watch --parallel",
    "compile": "node_modules/.bin/lerna exec  --stream -- tsc",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "pod": "cd ios && pod install && cd ..",
    "start": "react-native start",
    "web": "yarn compile && react-scripts start",
    "build": "CI=false react-scripts build",
    "eject": "react-scripts eject",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.10.3",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.4.2",
    "@react-navigation/stack": "^5.3.9",
    "@xstate/react": "^0.8.1",
    "color": "^3.1.2",
    "lodash": "^4.17.15",
    "react": "16.13.1",
    "react-native": "0.63.0",
    "react-native-gesture-handler": "^1.6.1",
    "react-native-reanimated": "^1.9.0",
    "react-native-safe-area-context": "^3.0.3",
    "react-native-screens": "^2.8.0",
    "react-native-svg": "^12.1.0",
    "react-native-web": "^0.13.3",
    "react-redux": "^7.2.0",
    "redux": "^4.0.0-0",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "2.3.0",
    "string-to-color": "^2.1.3",
    "xstate": "^4.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-flow-strip-types": "^7.10.1",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^24.0.24",
    "@types/lodash": "^4.14.155",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-native": "^0.62.0",
    "@types/react-redux": "^7.1.9",
    "@types/react-test-renderer": "16.9.2",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "lerna": "^3.22.1",
    "metro-react-native-babel-preset": "^0.58.0",
    "prettier": "^2.0.4",
    "pretty-quick": "^1.11.1",
    "react-docgen-typescript-loader": "^3.7.2",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "react-test-renderer": "16.11.0",
    "typescript": "^3.9.7",
    "webpack": "4.42.0"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
